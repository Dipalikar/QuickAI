{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],

  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs20.x",
      "includeFiles": "node_modules/canvas/**"
    }
  }
}
